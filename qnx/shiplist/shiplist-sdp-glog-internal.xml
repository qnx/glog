<?xml version="1.0" encoding="UTF-8"?>
<shiplist>
	<id>com.qnx.${baselineId}.osr.glog.internal</id>
	<name>glog 8.0 Library</name>
	<description>glog - Google Logging Library</description>
	<property key="f.support" value="Custom"/>
	<property key="f.swtype" value="III"/>
	<property key="f.engage" value="B"/>
	<category>OSR</category>

	<property key="com.qnx.internal" value="true"/>
	<!-- don't generate provides/requires tags for internal packages-->
	<requires value="suppress-library: autogen"/>
	<provides value="suppress-library: autogen"/>

	<file name="${targetfolder}/[ARCH:aarch64le,x86_64]/usr/lib/libglogd.so.0.6.0" />
	<file name="${targetfolder}/[ARCH:aarch64le,x86_64]/usr/lib/libglogd.so.1" />
	<file name="${targetfolder}/[ARCH:aarch64le,x86_64]/usr/lib/libglogd.so" />
	<file name="${targetfolder}/[ARCH:aarch64le,x86_64]/usr/lib/libgmock_main.so" />
	<asset dest="${targetfolder}/usr/include/glog/" />
	<asset dest="${targetfolder}/usr/include/gtest/" />
</shiplist>
